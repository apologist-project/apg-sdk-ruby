module Apologist
  module Models
    module Store
      type store_order =
        {
          id: Integer,
          complete: bool,
          pet_id: Integer,
          quantity: Integer,
          ship_date: Time,
          status: Apologist::Models::Store::StoreOrder::status
        }

      class StoreOrder < Apologist::Internal::Type::BaseModel
        attr_reader id: Integer?

        def id=: (Integer) -> Integer

        attr_reader complete: bool?

        def complete=: (bool) -> bool

        attr_reader pet_id: Integer?

        def pet_id=: (Integer) -> Integer

        attr_reader quantity: Integer?

        def quantity=: (Integer) -> Integer

        attr_reader ship_date: Time?

        def ship_date=: (Time) -> Time

        attr_reader status: Apologist::Models::Store::StoreOrder::status?

        def status=: (
          Apologist::Models::Store::StoreOrder::status
        ) -> Apologist::Models::Store::StoreOrder::status

        def initialize: (
          ?id: Integer,
          ?complete: bool,
          ?pet_id: Integer,
          ?quantity: Integer,
          ?ship_date: Time,
          ?status: Apologist::Models::Store::StoreOrder::status
        ) -> void

        def to_hash: -> {
          id: Integer,
          complete: bool,
          pet_id: Integer,
          quantity: Integer,
          ship_date: Time,
          status: Apologist::Models::Store::StoreOrder::status
        }

        type status = :placed | :approved | :delivered

        module Status
          extend Apologist::Internal::Type::Enum

          PLACED: :placed
          APPROVED: :approved
          DELIVERED: :delivered

          def self?.values: -> ::Array[Apologist::Models::Store::StoreOrder::status]
        end
      end
    end
  end
end
