module Apologist
  module Resources
    class Pet
      def create: (
        name: String,
        photo_urls: ::Array[String],
        ?id: Integer,
        ?category: Apologist::PetAPI::Category,
        ?status: Apologist::Models::PetAPI::status,
        ?tags: ::Array[Apologist::PetAPI::Tag],
        ?request_options: Apologist::request_opts
      ) -> Apologist::PetAPI

      def retrieve: (
        Integer pet_id,
        ?request_options: Apologist::request_opts
      ) -> Apologist::PetAPI

      def update: (
        name: String,
        photo_urls: ::Array[String],
        ?id: Integer,
        ?category: Apologist::PetAPI::Category,
        ?status: Apologist::Models::PetAPI::status,
        ?tags: ::Array[Apologist::PetAPI::Tag],
        ?request_options: Apologist::request_opts
      ) -> Apologist::PetAPI

      def delete: (
        Integer pet_id,
        ?request_options: Apologist::request_opts
